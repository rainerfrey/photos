<div class="panel panel-default">
  <div class="panel-heading">
    <h4 class="panel-title"><a href="#metadataTable" data-toggle="collapse">EXIF Data</a></h4>
  </div>
  <div class="panel-body collapse" id="metadataTable">
    <table class="table table-condensed">
      <tbody>
      {{#each-in metadata as |key value|}}
        <tr><td>{{key}}</td><td>{{value}}</td></tr>
      {{/each-in}}
      </tbody>
    </table>
  </div>
</div>